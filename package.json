{
  "name": "tanstack-start-system",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "npx convex dev --once --typecheck=disable && npm run db:generate && concurrently -r npm:dev:web npm:dev:db",
    "dev:web": "vite dev",
    "dev:db": "convex dev --typecheck=disable",
    "dev:db:reset": "npx convex dev --configure=new",
    "build": "npm run db:generate && vite build && tsc --noEmit",
    "build:prod": "NODE_ENV=production npm run db:generate && vite build --mode production",
    "start": "vite start",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "auth:start": "docker-compose up -d",
    "auth:stop": "docker-compose down",
    "auth:logs": "docker-compose logs -f postgres",
    "auth:clean": "docker-compose down -v && docker-compose up -d",
    "auth:generate": "DATABASE_URL=\"postgresql://geenius_user:geenius_password@localhost:5433/geenius_auth\" npx @better-auth/cli generate --config ./src/features/system/auth/lib/auth-config.generate.ts",
    "auth:migrate": "DATABASE_URL=\"postgresql://geenius_user:geenius_password@localhost:5433/geenius_auth\" prisma migrate dev",
    "auth:migrate:deploy": "DATABASE_URL=\"postgresql://geenius_user:geenius_password@localhost:5433/geenius_auth\" prisma migrate deploy",
    "auth:push": "DATABASE_URL=\"postgresql://geenius_user:geenius_password@localhost:5433/geenius_auth\" prisma db push",
    "auth:reset": "DATABASE_URL=\"postgresql://geenius_user:geenius_password@localhost:5433/geenius_auth\" prisma migrate reset",
    "auth:studio": "DATABASE_URL=\"postgresql://geenius_user:geenius_password@localhost:5433/geenius_auth\" prisma studio",
    "auth:seed": "DATABASE_URL=\"postgresql://geenius_user:geenius_password@localhost:5433/geenius_auth\" prisma db seed",
    "db:generate": "prisma generate",
    "stop": "lsof -ti:3000 | xargs kill -9 2>/dev/null; lsof -ti:3001 | xargs kill -9 2>/dev/null; lsof -ti:3210 | xargs kill -9 2>/dev/null; lsof -ti:3211 | xargs kill -9 2>/dev/null; lsof -ti:3212 | xargs kill -9 2>/dev/null; echo 'Processes stopped (ports 3000, 3001, 3210, 3211, 3212)'",
    "error": "pnpm tsc --noEmit",
    "validate:translations": "tsx scripts/validate-translations.ts",
    "error:codes": "pnpm tsc --noEmit 2>&1 | grep -oE 'TS[0-9]+' | sort | uniq -c | sort -rn",
    "error:filter": "pnpm tsc --noEmit 2>&1 | grep"
  },
  "dependencies": {
    "@ai-sdk/gateway": "^2.0.9",
    "@ai-sdk/provider": "^2.0.0",
    "@aws-sdk/client-ses": "^3.933.0",
